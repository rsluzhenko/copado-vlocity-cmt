{
  "Vlocity": {
    "rootfolder": "vlocity custom/**",
    "defaultFolder": "vlocity/**"
  },
  "SFDX": {
    "rootfolder": [
      "force-app/**",
      "sfdx custom/**"
    ],
    "config": {
      "filepath": "sfdx-project.json",
      "property": "packageDirectories[].path"
    },
    "defaultFolder": "force-app",
    "exclude": {
      "filePattern": [
        "**/__tests__/**",
        "**/.eslintrc.json",
        "**/jsconfig.json",
        "**/excluded files/**"
      ]
    }
  },
  "Other": {
    "default": true
  }
}